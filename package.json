{
  "name": "astraforge-ide",
  "displayName": "AstraForge IDE V3 üöÄ‚ú®",
  "description": "Visionary V3: Spec-driven automation, secure API orchestration, and environment-aware multi-LLM collaboration",
  "version": "3.0.0",
  "publisher": "astraforge",
  "engines": {
    "vscode": "^1.102.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astraforge/astraforge-ide.git"
  },
  "categories": [
    "Extension Packs"
  ],
  "main": "./extension/out/extension.js",
  "files": [
    "extension/out/**/*",
    "media/**/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "astraforge.projectIgnition",
          "name": "Project Ignition"
        },
        {
          "id": "astraforge.apiTester",
          "name": "API Tester"
        },
        {
          "id": "astraforge.metaLearning",
          "name": "üß† Meta-Learning"
        },
        {
          "id": "astraforge.emergentBehavior",
          "name": "üåü Emergent Behavior"
        },
        {
          "id": "astraforge.quantumDecision",
          "name": "üîÆ Quantum Decision Making"
        },
        {
          "id": "astraforge.agentEvolution",
          "name": "üß¨ Agent Evolution"
        },
        {
          "id": "astraforge.selfModification",
          "name": "‚öôÔ∏è Self-Modification"
        }
      ]
    },
    "commands": [
      {
        "command": "astraforge.testAPIs",
        "title": "Test APIs",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.showMetaLearning",
        "title": "Show Meta-Learning Dashboard",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.showEmergentBehavior",
        "title": "Show Emergent Behavior Dashboard",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.showQuantumDecision",
        "title": "Show Quantum Decision Making Dashboard",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.showAgentEvolution",
        "title": "Show Agent Evolution Dashboard",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.showSelfModification",
        "title": "Show Self-Modification Dashboard",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.evolveAgents",
        "title": "Evolve Agent Population",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.specializeAgents",
        "title": "Specialize Agents",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.transferKnowledge",
        "title": "Transfer Knowledge Between Agents",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.optimizeEvolution",
        "title": "Optimize Evolution Parameters",
        "category": "AstraForge"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "type-check": "tsc --noEmit",
    "test:api": "node out/testing/apiTesterCLI.js",
    "test:codacy": "node scripts/run-codacy-tests.cjs",
    "package": "vsce package",
    "clean": "rimraf out dist coverage .nyc_output",
    "agentnexus:build": "npm run compile && node ./extension/out/cli/runAgentNexusBuildPrompt.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.4.5",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vscode/vsce": "^2.32.0",
    "eslint": "^9.32.0",
    "fast-check": "^4.3.0",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@huggingface/inference": "^4.6.1",
    "axios": "^1.11.0",
    "commander": "^14.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "simple-git": "^3.19.1",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1"
  }
}
