{
  "name": "astraforge-ide",
  "displayName": "AstraForge IDE",
  "description": "The Sovereign AI IDE with Multi-Agent Consensus",
  "version": "1.0.0",
  "main": "dist/main/main/main.js",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/up2itnow/AstraForge.git"
  },
  "engines": {
    "node": "^18"
  },
  "categories": [
    "Other"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest --config jest.config.cjs --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "review": "node scripts/biWeeklyReview.js",
    "audit": "node scripts/quarterlyAudit.js",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:watch:coverage": "jest --watch --coverage",
    "test:fix": "node -r ts-node/register tests/test-fix-loop.ts",
    "test:generate": "node -r ts-node/register tests/auto-test-generator.ts",
    "test:diagnostic": "node -r ts-node/register tests/diagnostic.ts",
    "test:ci": "jest --config jest.config.cjs --ci --coverage",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "precommit": "npm run typecheck && npm run lint && npm test",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on tcp:5173 && tsc -p tsconfig.electron.json && electron .",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:vite": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build": "npm run build:vite && npm run build:electron",
    "pack": "electron-builder",
    "dist": "npm run build && electron-builder"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.2",
    "@elastic/elasticsearch": "^9.1.1",
    "@jest/globals": "^30.2.0",
    "@octokit/rest": "^22.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.17",
    "axe-core": "^4.11.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "eslint": "^8.0.0",
    "eslint-plugin-jsdoc": "^61.2.1",
    "jest": "^29.0.0",
    "jsdoc": "^4.0.5",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.25.0",
    "tailwindcss": "^3.4.10",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  },
  "types": "./out/extension.d.ts",
  "bugs": {
    "url": "https://github.com/up2itnow/AstraForge/issues"
  },
  "homepage": "https://github.com/up2itnow/AstraForge#readme",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@lancedb/lancedb": "^0.22.3",
    "@langchain/core": "^1.0.5",
    "@monaco-editor/react": "^4.7.0",
    "@types/axios": "^0.9.36",
    "@types/crypto-js": "^4.2.2",
    "@types/lru-cache": "^7.10.9",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "2.0.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@types/yargs": "^17.0.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "@xyflow/react": "^12.9.3",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "fast-check": "3.13.0",
    "framer-motion": "^12.23.24",
    "helmet": "^8.1.0",
    "langchain": "^1.0.4",
    "lru-cache": "^10.0.0",
    "lucide-react": "^0.554.0",
    "mathjs": "11.11.0",
    "node-pty": "^1.0.0",
    "openai": "^4.104.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "simple-git": "^3.30.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "supertest": "6.3.0",
    "uuid": "^9.0.0",
    "ws": "^8.14.0",
    "yargs": "^17.7.0",
    "zod": "^3.25.32"
  },
  "build": {
    "appId": "com.astraforge.ide",
    "productName": "AstraForge IDE",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "category": "Development"
    }
  },
  "productName": "AstraForge"
}
