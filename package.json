{
  "name": "astraforge-ide",
  "displayName": "AstraForge IDE",
  "description": "Advanced AI-powered VS Code extension for quantum-enhanced development",
  "version": "0.5.0",
  "publisher": "up2itnow",
  "type": "module",
  "main": "./out/extension.js",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/up2itnow/AstraForge.git"
  },
  "engines": {
    "vscode": "^1.85.0",
    "node": "^18"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "AI",
    "quantum",
    "development"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:astraforge.projectIgnition",
    "onCommand:astraforge.setupWizard",
    "onCommand:astraforge.apiTester",
    "onCommand:astraforge.health",
    "onCommand:astraforge.monitor",
    "onCommand:astraforge.collaborateSession",
    "onCommand:astraforge.gitSync",
    "onCommand:astraforge.llmConsensus",
    "onCommand:astraforge.emergentDetect",
    "onCommand:astraforge.quantumSim",
    "onCommand:astraforge.rbacManage",
    "onCommand:astraforge.scalingConfig",
    "onCommand:astraforge.securityAudit",
    "onCommand:astraforge.perfOptimize"
  ],
  "contributes": {
    "commands": [
      {
        "command": "astraforge.projectIgnition",
        "title": "Project Ignition",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.setupWizard",
        "title": "Setup Wizard",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.apiTester",
        "title": "API Tester",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.health",
        "title": "Health Check",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.monitor",
        "title": "Monitor KPIs",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.collaborateSession",
        "title": "Start Collaboration Session",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.gitSync",
        "title": "Git Sync",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.llmConsensus",
        "title": "LLM Consensus Query",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.emergentDetect",
        "title": "Detect Emergent Behavior",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.quantumSim",
        "title": "Run Quantum Simulation",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.rbacManage",
        "title": "Manage RBAC",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.scalingConfig",
        "title": "Configure Scaling",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.securityAudit",
        "title": "Run Security Audit",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.perfOptimize",
        "title": "Optimize Performance",
        "category": "AstraForge"
      },
      {
        "command": "astraforge.telemetryView",
        "title": "Open Telemetry Panel",
        "category": "AstraForge"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "astraforge.projectIgnition",
          "group": "navigation",
          "when": "resourceExtname == .ts || resourceExtname == .js"
        },
        {
          "command": "astraforge.gitSync",
          "group": "1_modification",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "astraforge.apiTester",
          "group": "z_commands"
        },
        {
          "command": "astraforge.llmConsensus",
          "group": "z_commands"
        }
      ],
      "commandPalette": [
        {
          "command": "astraforge.projectIgnition"
        },
        {
          "command": "astraforge.monitor"
        }
      ]
    },
    "keybindings": [
      {
        "command": "astraforge.projectIgnition",
        "key": "ctrl+shift+i",
        "mac": "cmd+shift+i",
        "when": "editorTextFocus"
      },
      {
        "command": "astraforge.health",
        "key": "ctrl+shift+h",
        "mac": "cmd+shift+h",
        "when": "editorTextFocus"
      },
      {
        "command": "astraforge.monitor",
        "key": "ctrl+shift+m",
        "mac": "cmd+shift+m",
        "when": "editorTextFocus"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "astraforge-sidebar",
          "title": "AstraForge",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "astraforge-sidebar": [
        {
          "id": "astraforge.telemetry",
          "name": "Telemetry",
          "type": "webview",
          "icon": "$(graph)"
        },
        {
          "id": "astraforge.llmPanel",
          "name": "Multi-LLM Panel",
          "type": "webview",
          "icon": "$(bot)"
        },
        {
          "id": "astraforge.monitorView",
          "name": "KPIs Monitor",
          "type": "webview",
          "icon": "$(dashboard)"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest --config jest.config.cjs --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "review": "node scripts/biWeeklyReview.js",
    "audit": "node scripts/quarterlyAudit.js",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:watch:coverage": "jest --watch --coverage",
    "test:fix": "node -r ts-node/register tests/test-fix-loop.ts",
    "test:generate": "node -r ts-node/register tests/auto-test-generator.ts",
    "test:diagnostic": "node -r ts-node/register tests/diagnostic.ts",
    "test:ci": "jest --config jest.config.cjs --ci --coverage",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "precommit": "npm run typecheck && npm run lint && npm test"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.10.2",
    "@elastic/elasticsearch": "^9.1.1",
    "@octokit/rest": "^22.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.130",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "axe-core": "^4.11.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.0.0",
    "eslint-plugin-jsdoc": "^61.2.1",
    "jest": "^29.0.0",
    "jsdoc": "^4.0.5",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.25.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "types": "./out/extension.d.ts",
  "bugs": {
    "url": "https://github.com/up2itnow/AstraForge/issues"
  },
  "homepage": "https://github.com/up2itnow/AstraForge#readme",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.0",
    "@lancedb/lancedb": "^0.22.3",
    "@langchain/core": "^1.0.5",
    "@types/axios": "^0.9.36",
    "@types/crypto-js": "^4.2.2",
    "@types/lru-cache": "^7.10.9",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "2.0.0",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@types/yargs": "^17.0.0",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "fast-check": "3.13.0",
    "langchain": "^1.0.4",
    "lru-cache": "^10.0.0",
    "mathjs": "11.11.0",
    "openai": "^4.20.0",
    "simple-git": "^3.30.0",
    "socket.io": "^4.7.0",
    "supertest": "6.3.0",
    "uuid": "^9.0.0",
    "vectordb": "^0.21.2",
    "ws": "^8.14.0",
    "yargs": "^17.7.0",
    "zod": "3.23.8"
  }
}